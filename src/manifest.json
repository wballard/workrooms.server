{
  "name": "Workrooms",
  "version": "0.0.2",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Workrooms"
  },
  "icons" : {
    "128": "images/icon.png"
  },
  "background": {
    "page": "pages/background.html"
  },
  "web_accessible_resources": [
    "all"
  ],
  "permissions": [
    "storage",
    "background",
    "identity",
    "pushMessaging",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}

